#include <Servo.h>

// Define the servos
Servo servo1;
Servo servo2;
Servo servo3;
Servo servo4;
Servo servo5;
Servo servo6;

void setup() {
  // Attach servos to pins
  servo1.attach(3);  // Pin 3
  servo2.attach(5);  // Pin 5
  servo3.attach(6);  // Pin 6
  servo4.attach(9);  // Pin 9
  servo5.attach(10); // Pin 10
  servo6.attach(11); // Pin 11
}

void loop() {
  // Move servos to different angles
  moveServos(90, 120, 60, 90, 120, 60); // Example angles, adjust as needed
  delay(1000); // Delay in milliseconds
  moveServos(60, 90, 120, 60, 90, 120); // Example angles, adjust as needed
  delay(1000); // Delay in milliseconds
}

void moveServos(int angle1, int angle2, int angle3, int angle4, int angle5, int angle6) {
  servo1.write(angle1);
  servo2.write(angle2);
  servo3.write(angle3);
  servo4.write(angle4);
  servo5.write(angle5);
  servo6.write(angle6);
}
